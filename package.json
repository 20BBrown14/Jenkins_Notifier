{
  "name": "jenkins-notifier",
  "version": "0.1.0",
  "description": "Chrome extension to track Jenkins Jobs",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.cerner.com/BB052256/Jenkins_Notifier.git"
  },
  "license": "UNLICENSED",
  "author": "Branden Brown",
  "scripts": {
    "build": "react-scripts build",
    "clean:all": "rimraf ./node_modules",
    "clean:install": "npm run clean:all && npm install",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "if [ -d './src' ]; then rimraf ./lib; fi",
    "compile:build": "babel src --out-dir lib --copy-files",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:test": "npm run lint && npm run test",
    "start": "react-scripts start",
    "test": "jest --config jestconfig.json",
    "clear_test_cache": "jest --clearCache",
    "update-snaps": "jest --updateSnapshot --config jestconfig.json"
  },
  "dependencies": {
    "deep-object-diff": "^1.1.0",
    "node-sass": "^4.10.0",
    "polyfill": "^0.1.0",
    "prop-types": "^15.6.2",
    "raf": "^3.4.1",
    "terra-app-delegate": "^2.8.0",
    "terra-arrange": "^2.31.1",
    "terra-base": "^3.29.0",
    "terra-button": "^2.31.1",
    "terra-card": "^2.30.0",
    "terra-clinical-application": "^2.9.0",
    "terra-form-field": "^2.31.0",
    "terra-form-input": "^1.30.0",
    "terra-icon": "^2.29.1",
    "terra-image": "^2.31.0",
    "terra-text": "^2.29.1",
    "xhr": "^2.5.0"
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-react-intl": "^2.0.3",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest-fetch-mock": "^2.0.1",
    "jest-image-snapshot": "^2.7.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "rimraf": "^2.6.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
